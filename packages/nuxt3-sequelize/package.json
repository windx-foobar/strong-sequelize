{
  "name": "@wxjs/nuxt3-sequelize",
  "version": "1.0.0",
  "type": "module",
  "keywords": [
    "sequelize",
    "nuxt3-sequelize"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windx-foobar/wxjs.git",
    "directory": "packages/nuxt3-sequelize"
  },
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist"],
  "scripts": {
