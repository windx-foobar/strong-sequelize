    "defu": "^6.1.3"
  },
  "devDependencies": {
    "sqlite3": "^5.1.6",
    "nuxt": "^3.9.0",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "@types/node": "^18.19.4",
    "@wxjs/sequelize-cli": "workspace:*",
    "@wxjs/h3-sequelize": "workspace:*",
    "@wxjs/shared": "workspace:*"
